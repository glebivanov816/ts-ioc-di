{
  "name": "ts-ioc-di",
  "version": "1.0.2",
  "description": "Typescript IoC container and DI",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV='production' npm run webpack",
    "dev": "cross-env NODE_ENV='development' npm run webpack",
    "test": "cross-env NODE_ENV='test' npm run webpack && npm run mocha",
    "mocha": "node node_modules/.bin/mocha .tmp/spec/spec.js",
    "webpack": "node node_modules/.bin/webpack --progress --config=./webpack"
  },
  "keywords": [
    "Typescript",
    "DI",
    "IoC"
  ],
  "author": "Gleb Ivanov<realglebivanov@gmail.com>",
  "license": "ISC",
  "repository": "github:glebivanov816/ts-ioc-di",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "cross-env": "^5.1.5",
    "mocha": "^5.1.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "webpack": "^3.12.0",
    "webpack-merge": "^4.1.2",
    "webpack-typings-aliases-plugin": "0.0.5"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12",
    "typescript": "^2.8.3"
  }
}
